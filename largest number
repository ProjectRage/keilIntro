    AREA largest,code,readonly
 entry
 
 start
    mov r5,#6
    ldr r1,=value1
    ldr r2,[r1],#4
 loop
    ldr r4,[r1],#4
    cmp r2,r4
    bhi loop1
    mov r2,r4
    
 loop1
    subs r5,#1
    cmp r5,#0
    bne loop
    
    ldr r4,=result
    str r2,[r4]
    
    back b back
    nop
    nop
    
 value 1 dcd 0x444444444,0x22222222,0x11111111,0x33333333,0xAAAAAAAA,0x88888888,0x99999999
 
  AREA data2,data,readwrite
 
resuklt dcd 0x0

end

