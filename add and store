  AREA addition,code,readonly
  
entry 
    
start
    mov r5,#10
    mov r0,#0
    ldr r1,=value1
loop
    ldrh r3,[r1],#02
    add r0,r0,r3
    subs r5,#1
    cmp r5,#0
    BNE loop
    ldr r4,=result
    str r0,[r4]
    
jmp b jmp


value1 dcw 0x1111, 0x2222, 0x3333,0xAAAA,0xBBBB,0xCCCC

  AREA data1,data,readwrite
 result dcd 0x0
 end


  
